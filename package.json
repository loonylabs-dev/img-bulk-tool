{
  "name": "img-bulk-tool",
  "version": "1.0.0",
  "description": "Bildbearbeitungs-Tool für Zerlegung, Komprimierung und Größenänderung",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/server.js\" \"vite\"",
    "start": "node dist/server.js",
    "serve": "vite preview"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-optipng": "^8.0.0",
    "cors": "^2.8.5",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/sharp": "^0.32.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
}